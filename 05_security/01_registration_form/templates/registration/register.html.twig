{# templates/registration/register.html.twig #}
{% extends 'base.html.twig' %}
{% block body %}
    {{ form_start(form) }}
        {{ form_row(form.username) }}
        {{ form_row(form.email) }}
        {{ form_row(form.plainPassword.first) }}
        {{ form_row(form.plainPassword.second) }}
        {{ form_row(form.termsAccepted) }}
        {{ form_row(form.submit) }}        
    {{ form_end(form) }}
{% endblock %}
